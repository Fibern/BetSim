{
    "https_port": 443,
    "Kestrel": {
    "Certificates": {
        "Default": {
            "Path": "/etc/ssl/certs/fullchain.pem",
            "KeyPath": "/etc/ssl/certs/privkey.pem"
          }
      }
    },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}